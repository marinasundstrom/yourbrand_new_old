{
    "Azure": {
        "AppConfig": {
            "Name": "yourbrand-appconfiguration"
        },
        "KeyVault": {
            "Name": "yourbrand-keyvault"
        },
        "ServiceBus": {
            "Namespace": "yourbrand-servicebus"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "yourbrand-carts-svc": {
                "ClusterId": "yourbrand-carts-svc",
                "Match": {
                    "Path": "/catalog/{**catch-all}"
                },
                "Transforms": [
                    {
                        "PathPattern": "{**catchall}"
                    }
                ]
            }
        },
        "Clusters": {
            "yourbrand-carts-svc": {
                "Destinations": {
                    "destination1": {
                        "Address": "http://yourbrand-carts-svc/"
                    }
                }
            }
        }
    }
}