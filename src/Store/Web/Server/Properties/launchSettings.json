      "applicationUrl": "https://localhost:7188;http://localhost:5153",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "yourbrand-catalog-svc-url": "https://catalog-svc",
        "yourbrand-carts-svc-url": "https://carts-svc"
      },
      "dotnetRunMessages": true
