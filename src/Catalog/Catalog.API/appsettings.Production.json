{
    "Azure": {
        "AppConfig": {
            "Name": "yourbrand-appconfiguration"
        },
        "KeyVault": {
            "Name": "yourbrand-keyvault"
        },
        "StorageAccount": {
            "Name": "yourbrandstorage"
        },
        "ServiceBus": {
            "Namespace": "yourbrand-servicebus"
        }
    },
    "ReverseProxy": {
        "Routes": {
            "content": {
                "ClusterId": "cluster1",
                "Match": {
                    "Path": "/images/{*any}"
                }
            }
        },
        "Clusters": {
            "cluster1": {
                "Destinations": {
                    "myDestination": {
                        "Address": "https://yourbrandstorage.blob.core.windows.net"
                    }
                },
                "LoadBalancingPolicy": "PowerOfTwoChoices"
            }
        }
    }
}